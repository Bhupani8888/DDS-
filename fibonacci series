#include <iostream>
using namespace std;
int Fib(int num){
    if(num==0){
        return 0;
    }
    if(num==1){
        return 1;
    }
    else {
        return Fib(num-1)+Fib(num-2);
    }
}
int main(){
    int n;
    cout<<"enter a number:";
    cin>>n;
    for(int i=0 ; i<n ; i++) {
        cout<<Fib(i)<<" ";
    }
}
    
